<% if flash[:errors] %>
<h1>Uh oh! You have errors. See below: </h1>
<ul>
    <% flash[:errors].each do |error|  %>
        <li><%= error %></li>
    <% end %>
</ul>
<% end %>



<%= form_for @artist do |f| %>
    Name: <%= f.text_field :name %><br />
    Title: <%= f.text_field :title %><br />
    Age: <%= f.number_field :age %><br />
    <%= f.submit %>
    <% end %>